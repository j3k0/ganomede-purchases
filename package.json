{
  "name": "ganomede-purchases",
  "version": "1.0.0",
  "api": "purchases/v1",
  "description": "NodeJS purchases module API",
  "main": "build/index.js",
  "scripts": {
    "prepublish": "npm run snyk-protect && npm run build && npm run lint && npm run test",
    "start": "node -r source-map-support/register build/index.js",
    "startw": "nodemon --exec \"npm start\" --watch build",
    "test": "FOVEA_BILLING_SECRET_KEY=1 npx mocha --colors --require ts-node/register --extensions ts tests/*.test.ts tests/**/*.test.ts | bunyan -l error -o short --color",
    "testw": "nodemon --ext ts --watch src/ --watch tests/ --watch config.ts --exec \"npm test\"",
    "coverage": "LOG_LEVEL=FATAL FOVEA_BILLING_SECRET_KEY=12345678 istanbul cover _mocha -- -b tests/helper.js tests/**/*.test.js && istanbul report",
    "lint": "eslint src/ tests/ --ext .ts",
    "snyk-protect": "snyk protect",
    "trans": "tsc -b -v -i -w",
    "build": "tsc",
    "buildw": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j3k0/ganomede-purchases.git"
  },
  "keywords": [
    "ganomede",
    "purchases",
    "subscription",
    "billing"
  ],
  "author": "Fovea <contact@fovea.cc>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/j3k0/ganomede-purchases/issues"
  },
  "homepage": "https://github.com/j3k0/ganomede-purchases#readme",
  "devDependencies": {
    "@types/async": "^2.4.2",
    "@types/bunyan": "^1.8.7",
    "@types/chai": "^3.5.2",
    "@types/eslint": "^7.28.2",
    "@types/istanbul": "^0.4.30",
    "@types/lodash": "^4.14.176",
    "@types/mocha": "^9.0.0",
    "@types/node-statsd": "^0.1.3",
    "@types/nodemon": "^1.19.1",
    "@types/redis": "^2.8.32",
    "@types/restify": "^8.4.2",
    "@types/restify-errors": "^4.3.3",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "chai": "^3.5.0",
    "eslint": "^8.4.1",
    "istanbul": "^0.4.5",
    "mocha": "^9.1.3",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0",
    "testdouble": "^3.16.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "async": "^2.1.2",
    "bunyan": "^1.8.1",
    "curtain-down": "^1.0.0",
    "ganomede-base-client": "^1.1.0",
    "lodash": "^4.17.2",
    "node-statsd": "^0.1.1",
    "redis": "^2.6.5",
    "restify": "^8.5.1",
    "restify-clients": "^4.0.0",
    "restify-errors": "^8.0.2",
    "snyk": "^1.30.1",
    "source-map-support": "^0.5.21"
  },
  "snyk": true
}
